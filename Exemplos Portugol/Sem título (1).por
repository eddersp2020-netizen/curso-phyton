programa {
  funcao inicio() {
    //1 Passo - Declarar as variaveis
    real valor1, porcentagem, resultado

    //2 Passo - Entrada
    escreva("Digite o primeiro valor: ")
    leia(valor1)

    escreva("Digite o segundo valor: ")
    leia(porcentagem)
    
  
    //3 Passo - Processamento
   resultado = valor1 * (porcentagem /100)

